<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gastell_gehr_haberl.studinazor">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Stundenplan"></activity>
        <activity android:name=".ToDoListe"></activity>
        <activity android:name=".Einkaufsliste"></activity>
        <activity android:name=".NewstickerActivity"></activity>
        <activity android:name=".StundenplanMontag"></activity>
        <activity android:name=".StundenplanDienstag"></activity>
        <activity android:name=".StundenplanMittwoch"></activity>
        <activity android:name=".StundenplanDonnerstag"></activity>
        <activity android:name=".StundenplanFreitag"></activity>

        <receiver android:name="com.gastell_gehr_haberl.studinazor.NotiPublisher"></receiver>
    </application>


</manifest>